//Ejercicio Números en JavaScript
let miAltura_cm = 168
let miAltura_m = 1.68
let miPeso_kg = 68.80

//Redondeo Altura en metros a número superior
let miAltura_m_redondeo = Math.ceil(miAltura_m)
console.log(miAltura_m_redondeo)
//Redondeo Peso KG a número Inferior
let miPeso_kg_redondeo = Math.floor(miPeso_kg)
console.log(miPeso_kg_redondeo)

////Una variable que contenga si "el máximo valor que se puede obtener en Javascript + 1" es igual al máximo valor que se puede obtener en Javascript
let max_value_JS_iguales = Number.MAX_VALUE + 1 === Number.MAX_VALUE
console.log(max_value_JS_iguales)
